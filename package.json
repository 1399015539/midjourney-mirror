{
  "name": "midjourney-mirror",
  "version": "1.0.0",
  "description": "Midjourney Mirror Website with CF Bypass",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build": "node scripts/build.js",
    "package": "node scripts/package.js",
    "test": "jest",
    "flaresolverr": "scripts/start-flaresolverr.sh",
    "flaresolverr:win": "scripts/start-flaresolverr.bat",
    "docker:build": "docker build -t midjourney-mirror .",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all"
  },
  "dependencies": {
    "express": "^4.18.2",
    "puppeteer": "^21.7.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "http-proxy-middleware": "^2.0.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1",
    "user-agents": "^1.0.1394",
    "tough-cookie": "^4.1.3",
    "node-cache": "^5.1.2",
    "winston": "^3.11.0",
    "rate-limiter-flexible": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "pkg": "^5.8.1",
    "archiver": "^6.0.1"
  },
  "pkg": {
    "scripts": "src/**/*.js",
    "assets": [
      "public/**/*",
      "views/**/*",
      "node_modules/puppeteer/.local-chromium/**/*"
    ],
    "targets": [
      "node18-linux-x64",
      "node18-macos-x64",
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "midjourney",
    "mirror",
    "cloudflare",
    "bypass",
    "proxy"
  ],
  "author": "Anonymous",
  "license": "MIT"
}