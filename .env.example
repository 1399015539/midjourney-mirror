# Midjourney Mirror Configuration

# Server Configuration
PORT=3000
NODE_ENV=development

# Test Mode (set to false for production with real Puppeteer)
TEST_MODE=true

# Midjourney Configuration
MIDJOURNEY_BASE_URL=https://www.midjourney.com
MIDJOURNEY_API_URL=https://api.midjourney.com

# FlareSolverr Configuration
FLARESOLVERR_URL=http://localhost:8191
FLARESOLVERR_ENABLED=true

# Proxy Configuration
PROXY_ENABLED=true
PROXY_ROTATION_INTERVAL=300000

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin123

# Browser Configuration (for production mode)
HEADLESS_BROWSER=true
BROWSER_TIMEOUT=30000
MAX_CONCURRENT_BROWSERS=5

# Cache Configuration
CACHE_TTL=3600000
MAX_CACHE_SIZE=1000

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/app.log